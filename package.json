{
  "name": "ngvn",
  "version": "0.0.0",
  "description": "Phased 2 of a simplified Jira clone built with Angular 9 and Akita - Now with NestJS and GraphQL backed",
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "nx": "nx",
    "start": "ng serve",
    "build": "ng build --prod=true",
    "test": "ng test",
    "lint": "nx workspace-lint && ng lint",
    "e2e": "ng e2e",
    "gql-codegen": "graphql-codegen --config .graphqlconfig.yml",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "start:ng": "ng serve jira-clone",
    "start:api": "ng serve api",
    "start:functions": "ng serve firebase-functions",
    "build:functions:watch": "nx run firebase-functions:build-functions --watch",
    "build:functions:prod": "nx build firebase-functions",
    "prepare:functions:env": "firebase functions:config:unset env && firebase functions:config:set env=\"$(cat functions.env.json)\"",
    "prepare:functions:env:prod": "firebase functions:config:unset env && firebase functions:config:set env=\"$(cat functions.env.prod.json)\"",
    "deploy:functions": "npm run build:functions:prod && npm run prepare:functions:env && nx deploy firebase-functions",
    "deploy:functions:prod": "npm run build:functions:prod && npm run prepare:functions:env:prod && nx deploy firebase-functions",
    "netlify-build": "npm i && npm run build",
    "heroku-postbuild": "npm i && npm run build:back"
  },
  "main": "dist/apps/functions/main.js",
  "private": true,
